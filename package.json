{
  "name": "@get-dx/backstage-backend-plugin",
  "version": "2.0.0",
  "description": "Backstage backend plugin for DX! https://getdx.com",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "repository": "https://github.com/get-dx/backstage-backend-plugin",
  "author": "DX <developers@getdx.com>",
  "license": "Apache-2.0",
